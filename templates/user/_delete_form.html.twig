<form method="post" action="{{ path('user_delete', {'id': user.id}) }}" onsubmit="return confirm('Esta seguro que desea borrar el usuario?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="w-10 btn btn-primary btn-lg">Borrar</button>
</form>
